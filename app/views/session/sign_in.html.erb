<h1 class="text-center">Welcome back to the Crypto News Center </h1> 
<br>
<br>
<br>
<br>
<h2 class="text-center"> Sign In </h2>
<br>
<br>
<div class="text-center">
<%= form_with(url: "/session") do |f|%>
    <div class="mb-3">
    <%= f.email_field :email, autofocus: true, placeholder: "Email"%>
    </div>
    <div class="mb-3">
    <%= f.password_field :password, placeholder: "Password"%>
    </div>
    <br>
    <div class="mb-3">
    <%= f.submit "Sign In"%>
    </div>
<%end%>
</div>